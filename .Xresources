! urxvt settings 
urxvt*font: xft:Bitstream Vera Sans Mono:pixelsize=14:antialias=true,xft:DejaVu Sans:pixelsize=14, xft:DejaVu Sans Mono:pixelsize=14
urxvt*boldFont:
urxvt*letterSpace: 0
urxvt*scrollBar: false
urxvt*pointerBlank: true
urxvt*depth: 32
urxvt*internalBorder: 32
urxvt*iso14755: false
urxvt*iso14755_52: false


! urxvt extensions
urxvt*perl-ext-common: fullscreen,resize-font

!! fullscreen
urxvt*keysym.F11: perl:fullscreen:switch

! rxvt-256color is often rejected
urxvt*termName: xterm-256color

! colors
*.foreground: #F8F8F2
*.background: [70]#111111
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15: #E6E6E6
